			"type": "css",
			"value": ".bootstrap-switch-id-contact-terms"
		},
		"continue_btn": {
			"type": "css",
			"value": ".contact-info.form-inputs button"
		},
		"name": {
			"type": "id",
			"value": "contact-name"
		},
		"email": {
			"type": "id",
			"value": "contact-email_address"
		},
		"enquiry": {
			"type": "id",
			"value": "contact-content"
		},
		"error_name": {
			"type": "xpath",
			"value": "//input[@id='contact-name']/following-sibling::div/div[@class='required-message']"
		},
		"error_email": {
			"type": "xpath",
			"value": "//input[@id='contact-email_address']/following-sibling::div/div[@class='required-message']"
		},
		"error_enquiry": {
			"type": "xpath",
			"value": "//textarea[@id='contact-content']/following-sibling::div/div[@class='required-message']"
		},
		"message": {
			"type":"css",
			"value":"div[data-name*='ContactForm'] div"
		}
